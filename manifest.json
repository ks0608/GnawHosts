{
    "name": "Gnaw Hosts",
    "description": "Set proxy. You can use this app like hosts file.",
    "icons": {
        "16": "icon/icon_16.png",
        "48": "icon/icon_48.png",
        "128": "icon/icon_128.png"
    },
    "version": "1.0.0",
    "permissions": [
        "proxy",
        "http://*/*",
        "https://*/*"
    ],
    "app": {
        "launch": {
            "local_path": "set.html"
        }
    },
    "manifest_version": 2
}

